<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="RedirectOldChrisBarrPortfolioToWebsite" stopProcessing="true">
          <match url=".*" />
          <conditions logicalGrouping="MatchAny">
            <add input="{HTTP_HOST}{REQUEST_URI}" pattern="(.*)/chris-barr-portfolio(.*)$" />
          </conditions>
          <action type="Redirect" appendQueryString="true" url="http://chrisbarr.me?redirect=landmarkanalytics.com"
            redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>