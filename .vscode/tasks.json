{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": ["clean", "${workspaceFolder}/landmark-public-web.csproj"],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/landmark-public-web.csproj"],
      "problemMatcher": "$msCompile",
      "dependsOn": ["clean"]
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "args": ["watch", "run", "--project", "${workspaceFolder}/landmark-public-web.csproj"],
      "problemMatcher": "$msCompile",
      "dependsOn": ["clean"]
    }
  ]
}
